{
    "manifest_version": 3,
    "name": "Color-Picker",
    "description": "A simple color picker extension.",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icons/icon.png",
        "default_title": "Color Picker"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    }
}